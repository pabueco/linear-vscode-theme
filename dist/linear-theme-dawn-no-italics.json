{
  "name": "Linear Theme - Dawn (No italics)",
  "type": "dark",
  "colors": {
    "terminal.ansiGreen": "#4CB782",
    "terminal.ansiBrightGreen": "#97D484",
    "terminal.ansiMagenta": "#cba0ff",
    "terminal.ansiYellow": "#ffcc6d",
    "terminal.ansiCyan": "#26B5CE",
    "terminal.ansiRed": "#eb5757",
    "foreground": "#cfc4d4",
    "icon.foreground": "#cfc4d4",
    "focusBorder": "#a84376",
    "contrastBorder": "#4b3d52",
    "errorForeground": "#eb5757",
    "selection.background": "#a84376",
    "button.background": "#a84376",
    "button.hoverBackground": "#bc578a",
    "progressBar.background": "#bc578a",
    "input.background": "#56455e",
    "input.border": "#4b3d52",
    "inputOption.activeBackground": "#a84376",
    "inputValidation.errorBorder": "#eb5757",
    "editor.background": "#2A222E",
    "editor.foreground": "#cfc4d4",
    "peekViewEditor.background": "#362b3b",
    "dropdown.background": "#362b3b",
    "dropdown.border": "#4b3d52",
    "sideBar.background": "#382A3C",
    "sideBar.border": "#4c3951",
    "sideBarSectionHeader.background": "#403446",
    "activityBar.background": "#382A3C",
    "activityBar.foreground": "#cfc4d4",
    "activityBar.border": "#4c3951",
    "activityBar.activeBorder": "#a84376",
    "activityBarBadge.background": "#a84376",
    "titleBar.activeBackground": "#362b3b",
    "titleBar.inactiveBackground": "#362b3b",
    "editor.selectionBackground": "#d2338369",
    "editor.selectionHighlightBackground": "#c5267642",
    "editor.wordHighlightBackground": "#ffffff00",
    "editor.wordHighlightBorder": "#d23383ff",
    "editor.wordHighlightStrongBackground": "#ffffff00",
    "editor.wordHighlightStrongBorder": "#d23383ff",
    "editor.hoverHighlightBackground": "#56455e",
    "editor.lineHighlightBackground": "#362b3b",
    "editor.lineHighlightBorder": "#362b3b",
    "editorWhitespace.foreground": "#56455e",
    "editorRuler.foreground": "#56455e",
    "editorIndentGuide.background": "#4b3d52",
    "editorIndentGuide.activeBackground": "#6b5775",
    "tree.indentGuidesStroke": "#604e6a",
    "tree.tableColumnsBorder": "#604e6a",
    "editorCursor.foreground": "#ffaee1",
    "terminalCursor.foreground": "#ffaee1",
    "editorLineNumber.foreground": "#604e6a",
    "editorLineNumber.activeForeground": "#cfc4d4",
    "editorWidget.background": "#362b3b",
    "editorSuggestWidget.highlightForeground": "#d16c9f",
    "editorSuggestWidget.selectedBackground": "#403446",
    "tab.activeBorderTop": "#a84376",
    "tab.activeBackground": "#3b3041",
    "tab.inactiveBackground": "#2b232f",
    "tab.border": "#4c3951",
    "editorGroupHeader.tabsBackground": "#2b232f",
    "editorGroupHeader.border": "#362b3b",
    "editorGroupHeader.tabsBorder": "#4c3951",
    "list.activeSelectionBackground": "#403446",
    "list.activeSelectionForeground": "#e580b3",
    "list.inactiveSelectionBackground": "#3b3041",
    "list.inactiveSelectionForeground": "#e580b3",
    "list.hoverBackground": "#403446",
    "list.focusBackground": "#403446",
    "gitDecoration.modifiedResourceForeground": "#26B5CE",
    "gitDecoration.deletedResourceForeground": "#eb5757",
    "gitDecoration.untrackedResourceForeground": "#4CB782",
    "gitDecoration.ignoredResourceForeground": "#6b5775",
    "gitDecoration.conflictingResourceForeground": "#F2994A",
    "panel.background": "#382A3C",
    "panel.border": "#4c3951",
    "panelTitle.activeBorder": "#a84376",
    "badge.background": "#a84376",
    "statusBar.background": "#362b3b"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.block",
        "comment.line",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#9e8aa8"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "entity.name.variable",
        "string constant.other.placeholder",
        "source.vue meta.object.member.ts meta.object.member.ts"
      ],
      "settings": {
        "foreground": "#ece8ee"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#ece8ee"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "constant.other.class",
        "variable.other.property",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#f3b7e9"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#eb5757"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "meta storage.type"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#7e89ec"
      }
    },
    {
      "name": "Keyword, Operator",
      "scope": [
        "keyword.definition"
      ],
      "settings": {
        "foreground": "#8e82f8"
      }
    },
    {
      "name": "Comparison",
      "scope": [
        "keyword.operator.comparison",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#a89fff"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword.operator.logical.soil"
      ],
      "settings": {
        "foreground": "unset"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "keyword.other.template",
        "keyword.other.substitution",
        "meta.brace",
        "punctuation.support.type.property-name",
        "expression.embbeded punctuation.definition.tag.begin.html",
        "expression.embbeded punctuation.definition.tag.end.html"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#5da6b8"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#5da6b8"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "punctuation.definition.binding-pattern.object.js"
      ],
      "settings": {
        "foreground": "#5da6b8"
      }
    },
    {
      "name": "Tag Wrappers",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#74829e"
      }
    },
    {
      "name": "HTML Text",
      "scope": [
        "text.html",
        "meta.jsx.children.js"
      ],
      "settings": {
        "foreground": "#bdc4cc"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "entity.name.type.namespace",
        "keyword.other"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#beb7ff"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control.flow",
        "keyword.control.export",
        "keyword.control.return",
        "meta.function.closure",
        "keyword.control.import",
        "keyword.control.from",
        "keyword.control.as",
        "keyword.other.namespace"
      ],
      "settings": {
        "fontStyle": "normal"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#83acf8"
      }
    },
    {
      "name": "Function and Method Call",
      "scope": [
        "meta.method.declaration meta.function-call entity.name.function",
        "meta.method-call entity.name.function",
        "meta.function-call entity.name.function",
        "variable.function",
        "support.function",
        "entity.name.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#1abdda"
      }
    },
    {
      "name": "Function and Method Definition",
      "scope": [
        "meta.function entity.name.function",
        "meta.method entity.name.method",
        "meta.method.declaration entity.name.function",
        "meta.definition.method entity.name.function"
      ],
      "settings": {
        "foreground": "#61e6fd"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#88e6d6"
      }
    },
    {
      "name": "Number, Constant, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#ffbf71"
      }
    },
    {
      "name": "String, Symbols, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#97D484"
      }
    },
    {
      "name": "String Quotes",
      "scope": [
        "string punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#95d581bd"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "entity.name.type.class",
        "support.type",
        "support.class",
        "new.expr meta.function-call entity.name.function",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "support.constant",
        "entity.name.type.namespace",
        "meta.tag.custom entity.name.tag.html",
        "source.vue invalid.illegal.unrecognized-tag.html"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#88e6d6"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type",
        "meta.return.type",
        "entity.name.type",
        "meta.use.php"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#88e6d6"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "meta.type.tuple meta.brace"
      ],
      "settings": {
        "fontStyle": "normal"
      }
    },
    {
      "name": "Namespaces and Classes",
      "scope": [
        "entity.other.inherited-class",
        "support.orther.namespace.use",
        "support.other.namespace"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#9ebecc"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#a8f2ff"
      }
    },
    {
      "name": "Attributes, HTML Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta entity.other.attribute-name",
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name",
        "meta.directive.vue keyword.control"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#cba0ff"
      }
    },
    {
      "name": "CSS Classes, IDs",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        "entity.name.tag.css"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#cba0ff"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#97D484"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#eb5757"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#26B5CE"
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "@include",
      "scope": [
        "keyword.control.at-rule"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#1abdda"
      }
    },
    {
      "name": "&",
      "scope": [
        "entity.name.tag.reference"
      ],
      "settings": {
        "foreground": "#5fdfff"
      }
    },
    {
      "name": "Property names",
      "scope": [
        "support.type.property-name.json",
        "meta.objectliteral",
        "source.vue meta.object.member.ts meta.object.member.ts meta.object.member.ts",
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#b1c5ca"
      }
    },
    {
      "name": "Attribute value unit",
      "scope": [
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#d3782e"
      }
    },
    {
      "name": "Attribute value unit",
      "scope": [
        "source.env"
      ],
      "settings": {
        "foreground": "#97D484"
      }
    },
    {
      "name": "Attribute value unit",
      "scope": [
        "text.html.markdown",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#ece8ee"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#88e6d6"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#97D484"
      }
    }
  ]
}