{
    "name": "linear-theme",
    "displayName": "Linear Theme",
    "description": "A set of pleasant themes inspired by Linear",
    "version": "0.5.0",
    "publisher": "pabue",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/pabueco/linear-vscode-theme.git"
    },
    "semanticHighlighting": true,
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "linear",
        "dark",
        "gray",
        "purple",
        "theme",
        "color-theme"
    ],
    "icon": "icon.png",
    "contributes": {
        "themes": [
            {
                "label": "Linear Theme - Dark",
                "uiTheme": "vs-dark",
                "path": "./dist/linear-theme-dark.json"
            },
            {
                "label": "Linear Theme - Dark Variant",
                "uiTheme": "vs-dark",
                "path": "./dist/linear-theme-dark-variant.json"
            },
            {
                "label": "Linear Theme - Dawn",
                "uiTheme": "vs-dark",
                "path": "./dist/linear-theme-dawn.json"
            },
            {
                "label": "Linear Theme - Pale",
                "uiTheme": "vs-dark",
                "path": "./dist/linear-theme-pale.json"
            },
            {
                "label": "Linear Theme - Midnight",
                "uiTheme": "vs-dark",
                "path": "./dist/linear-theme-midnight.json"
            },
            {
                "label": "Linear Theme - Light",
                "uiTheme": "vs",
                "path": "./dist/linear-theme-light.json"
            },
            {
                "label": "Linear Theme - Ash",
                "uiTheme": "vs",
                "path": "./dist/linear-theme-ash.json"
            }
        ]
    },
    "dependencies": {
        "lodash": "^4.17.21"
    },
    "scripts": {
        "dev": "nodemon src/scripts/build.js --ignore dist/"
    },
    "devDependencies": {
        "@ctrl/tinycolor": "^3.4.0",
        "chroma-js": "^2.1.2",
        "deepdash": "^5.3.9"
    }
}
