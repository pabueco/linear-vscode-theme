{
    "name": "linear-theme",
    "displayName": "Linear Theme",
    "description": "A pleasant theme inspired by Linear",
    "version": "0.4.0",
    "publisher": "pabue",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/pabueco/linear-vscode-theme.git"
    },
    "semanticHighlighting": true,
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "linear",
        "dark",
        "gray",
        "purple",
        "theme",
        "color-theme"
    ],
    "icon": "icon.png",
    "contributes": {
        "themes": [
            {
                "label": "Linear Theme - Dark",
                "uiTheme": "vs-dark",
                "path": "./dist/linear-theme-dark.json"
            },
            {
                "label": "Linear Theme - Dark Variant",
                "uiTheme": "vs-dark",
                "path": "./dist/linear-theme-dark-variant.json"
            }
        ]
    },
    "dependencies": {
        "lodash": "^4.17.21"
    },
    "scripts": {
      "dev": "nodemon src/scripts/build.js --ignore dist/"
    }
}
