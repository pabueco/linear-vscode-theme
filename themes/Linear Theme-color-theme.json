			"settings": {
				"foreground": "#eb5757"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
        "storage.type",
				"storage.modifier"
			],
			"settings": {
        "fontStyle": "",
				"foreground": "#7681df"
				// "foreground": "#26B5CE"
			}
		},
		{
			"name": "Storage",
			"scope": [
        "storage.type",
				"storage.modifier"
			],
			"settings": {
        // "fontStyle": "italic",
			}
		},
		{
			"name": "Operator, Misc",
